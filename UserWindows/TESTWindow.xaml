        Height="440"
        Width="600"
        >
    <Grid>
        <WrapPanel
            x:Name="IndexPanel"
            >

                <Grid
                    
                    HorizontalAlignment="Stretch"
                    >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="50"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>

                    <TextBlock Grid.Column="0" Text="NAME"  VerticalAlignment="Center"/>
                    <WrapPanel
    
                        Grid.Column="1">





                    </WrapPanel>


                </Grid>






        </WrapPanel>
    </Grid>
</Window>
